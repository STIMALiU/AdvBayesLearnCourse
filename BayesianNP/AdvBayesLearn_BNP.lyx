\begin_deeper
\begin_layout Itemize
Let 
\begin_inset Formula $B_{1},B_{2},...,B_{k}$
\end_inset

 be a partition of the outcome space 
\begin_inset Formula $\text{\Omega}$
\end_inset

.
\end_layout

\begin_layout Itemize
Let 
\begin_inset Formula $P(B_{1}),...,P(B_{k})$
\end_inset

 denote the distribution over the partition.
\end_layout

\begin_layout Itemize
Dirichlet distribution is a 
\series bold
distribution over a space of distributions
\series default
:
\begin_inset Formula 
\[
(P(B_{1}),...,P(B_{k}))\sim Dirichlet(\alpha P_{0}(B_{1}),...,\alpha P_{0}(B_{k}))
\]

\end_inset

where 
\begin_inset Formula $P_{0}$
\end_inset

 is a fixed probability measure (e.g.
 the 
\begin_inset Formula $N(0,1)$
\end_inset

 density).
\end_layout

\begin_layout Itemize
Dirichlet distribution is closed under summation or splitting of bins.
 
\end_layout

\begin_layout Itemize
Can be used to define a 
\series bold
stochastic process
\series default
 in a consistent way.
 Compare with GPs.
\end_layout

\begin_layout Itemize
A random probability measure 
\begin_inset Formula $P$
\end_inset

 follows a 
\series bold
Dirichlet process
\series default
 
\begin_inset Formula $P\sim DP(\alpha\cdot P_{0})$
\end_inset

 with base measure 
\begin_inset Formula $P_{0}$
\end_inset

 and precision parameter 
\begin_inset Formula $\alpha$
\end_inset

 iff 
\begin_inset Formula 
\[
(P(B_{1}),...,P(B_{k}))\sim Dirichlet(\alpha P_{0}(B_{1}),...,\alpha P_{0}(B_{k}))
\]

\end_inset

for any finite (measureable) partition 
\begin_inset Formula $B_{1},...,B_{k}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
The Dirichlet process - properties
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
If 
\begin_inset Formula $P\sim DP(\alpha P_{0})$
\end_inset

 then
\begin_inset Formula 
\[
P(B)\sim\mathrm{Beta}\left[\alpha P_{0}(B),\alpha\left(1-P_{0}(B)\right)\right],\;\text{for any }B\in\mathcal{B}
\]

\end_inset


\begin_inset Formula 
\[
E\left[P(B)\right]=P_{0}(B)
\]

\end_inset


\begin_inset Formula 
\[
\mathrm{Var}\left[P(B)\right]=P_{0}(B)\left[1-P_{0}(B)\right]/(1+\alpha)
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
Model
\series default

\begin_inset Formula 
\[
y_{i}|P\overset{iid}{\sim}P\;,\;\text{for }i=1,...,n
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
Prior
\series default

\begin_inset Formula 
\[
P\sim DP(\alpha P_{0})
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
Posterior
\series default
 for a 
\series bold
finite partition
\series default
, 
\begin_inset Formula $P(B_{1}),...,P(B_{k})|\mathbf{y}$
\end_inset

 is 
\begin_inset Formula 
\[
Dir\left(\alpha P_{0}(B_{1})+\sum_{i=1}^{n}1_{y_{i}\in B_{1}},...,\alpha P_{0}(B_{k})+\sum_{i=1}^{n}1_{y_{i}\in B_{k}}\right)
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
The Dirichlet process - properties
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize

\series bold
Posterior
\series default
 for the unknown probability distribution 
\begin_inset Formula $P$
\end_inset


\begin_inset Formula 
\[
P|y_{1},...,y_{n}\sim DP\left(\alpha P_{0}+\sum_{i=1}^{n}\delta_{y_{i}}\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
Since 
\begin_inset Formula 
\[
P(B)\sim Beta\left(\alpha P_{0}(B)+\sum_{i=1}^{n}1_{y_{i}\in B},\alpha(1-P_{0}(B))+\sum_{i=1}^{n}1_{y_{i}\in B^{c}}\right)
\]

\end_inset

so
\begin_inset Formula 
\[
E\left(P(B)|y_{1},...,y_{n}\right)=\left(\frac{\alpha}{\alpha+n}\right)P_{0}(B)+\left(\frac{n}{\alpha+n}\right)\sum_{i=1}^{n}\frac{1}{n}1_{y_{i}\in B^{c}}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Estimating a d.f.
 with a DP prior
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
If 
\begin_inset Formula $B=(-\infty,y]$
\end_inset

 then
\begin_inset Formula 
\[
E\left(F(y)|y_{1},...,y_{n}\right)=\left(\frac{\alpha}{\alpha+n}\right)F_{0}(y)+\left(\frac{n}{\alpha+n}\right)F_{n}(y)
\]

\end_inset


\end_layout

\begin_layout Itemize
where
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $F(y)$
\end_inset

 is the unknown d.f.
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{0}(y)$
\end_inset

 is the d.f.
 from 
\begin_inset Formula $P_{0}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{n}(y)=\frac{1}{n}\sum1_{y_{i}\leq y}$
\end_inset

 is the empirical d.f.
\begin_inset VSpace smallskip
\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
Note: under the DP posterior, 
\begin_inset Formula $F(\cdot)$
\end_inset

 is discrete with probability one.
 Not great for continuous data ...
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Itemize
This is true in general: 
\series bold
realisations from a DP are discrete with probability one
\series default
.
\end_layout

\end_deeper
