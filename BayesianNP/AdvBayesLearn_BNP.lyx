\begin_layout Plain Layout
Finite mixture models
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize

\series bold
Mixture of normals
\series default

\begin_inset Formula 
\[
p(y)=\sum_{j=1}^{k}\pi_{j}\cdot\phi(y;\mu_{j},\sigma_{j}^{2})
\]

\end_inset


\end_layout

\begin_layout Itemize
Use 
\series bold
allocation variables
\series default
: 
\begin_inset Formula $I_{i}=j$
\end_inset

 if 
\begin_inset Formula $y_{i}$
\end_inset

 comes from 
\begin_inset Formula $\phi(y;\mu_{j},\sigma_{j}^{2})$
\end_inset

.
\end_layout

\begin_layout Itemize
Let 
\begin_inset Formula $I=(I_{1},...,I_{n})'$
\end_inset

 and 
\begin_inset Formula $n_{j}=\sum_{i=1}^{n}\left(I_{i}=j\right)$
\end_inset

.
\end_layout

\begin_layout Itemize

\series bold
Gibbs sampling
\series default
 algorithm:
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\pi_{1},...,\pi_{k}$
\end_inset

 
\begin_inset Formula $|$
\end_inset

 
\begin_inset Formula $I,y\sim Dirichlet(a_{1}+n_{1},a_{2}+n_{2},...,a_{k}+n_{k})$
\end_inset


\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\sigma_{j}^{2}$
\end_inset

 
\begin_inset Formula $|$
\end_inset

 
\begin_inset Formula $I,y\sim Inv$
\end_inset

-
\begin_inset Formula $\chi^{2}$
\end_inset

 and 
\begin_inset Formula $\mu_{j}|I,\sigma_{j}^{2},y\sim N$
\end_inset

 for 
\begin_inset Formula $j=1,...,k$
\end_inset

.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $I_{i}$
\end_inset

 
\begin_inset Formula $|$
\end_inset

 
\begin_inset Formula $\pi,\mu,\sigma^{2},y\sim Multinomial(\omega_{i,1},...,\omega_{i,k})$
\end_inset

, 
\begin_inset Formula $i=1,...,n,$
\end_inset

 where
\begin_inset Formula 
\[
\omega_{i,j}=\frac{\pi_{j}\cdot\phi(y_{i};\mu_{j},\sigma_{j}^{2})}{\sum_{q=1}^{k}\pi_{q}\cdot\phi(y_{i};\mu_{q},\sigma_{q}^{2})}.
\]

\end_inset


\end_layout

\end_deeper
\end_deeper
