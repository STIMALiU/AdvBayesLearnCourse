\begin_deeper
\begin_layout Itemize

\series bold
Pitman-Yor process 
\series default
with parameters 
\begin_inset Formula $P_{0}$
\end_inset

, 
\begin_inset Formula $0\leq a<1$
\end_inset

 and 
\begin_inset Formula $b>-a$
\end_inset

:
\begin_inset Formula 
\[
P(\cdot)=\sum_{h=1}^{\infty}\pi_{h}\delta_{\theta_{i}}\quad\theta_{h}\overset{iid}{\sim}P_{0}
\]

\end_inset


\begin_inset Formula 
\[
\pi_{h}=V_{h}\prod_{\ell<h}(1-V_{\ell})
\]

\end_inset


\begin_inset Formula 
\[
V_{h}\overset{iid}{\sim}Beta(1-a,b+ha)
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
Probit stick-breaking 
\series default
with parameters 
\begin_inset Formula $\mu$
\end_inset

 and v
\begin_inset Formula $\sigma$
\end_inset

: 
\begin_inset Formula 
\[
P(\cdot)=\sum_{h=1}^{\infty}\pi_{h}\delta_{\theta_{i}}\quad\theta_{h}\overset{iid}{\sim}P_{0}
\]

\end_inset


\begin_inset Formula 
\[
\pi_{h}=V_{h}\prod_{\ell<h}(1-V_{\ell})
\]

\end_inset


\begin_inset Formula 
\[
V_{h}=\Phi(x_{h}),\;\text{ where \ensuremath{x_{h}\overset{iid}{\sim}N(\mu,\sigma^{2})}}
\]

\end_inset


\end_layout

\end_deeper
